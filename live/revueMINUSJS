         <script>
    function showDateTime() {
	var myDiv = document.getElementById("myDiv");
	const text = Intl.DateTimeFormat('en-US', {
	    timeZone: 'America/Chicago',
	    dateStyle: 'full',
	    timeStyle: 'medium'
	}).formatToParts(new Date()).map((p) => p.value).toSpliced(7, 1, ' ').reduce((x, y) => x + y);
	myDiv.innerText = text;
}
setInterval(showDateTime, 1000);
</script>
